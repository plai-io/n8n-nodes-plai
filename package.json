{
	"name": "n8n-nodes-plai",
	"version": "1.0.2",
	"description": "n8n community node for Plai - Receive leads from Meta and LinkedIn ads in real-time",
	"license": "MIT",
	"homepage": "https://github.com/plai-io/n8n-nodes-plai",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"plai",
		"facebook leads",
		"linkedin leads",
		"meta ads",
		"lead generation",
		"advertising",
		"marketing automation",
		"webhook",
		"crm"
	],
	"author": {
		"name": "Plai Team",
		"email": "support@plai.io"
	},
	"repository": {
